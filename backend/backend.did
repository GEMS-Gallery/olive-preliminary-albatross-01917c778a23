type Result = 
 variant {
   err: text;
   ok;
 };
type PredefinedItem = 
 record {
   icon: text;
   name: text;
 };
type Item = 
 record {
   category: text;
   completed: bool;
   icon: text;
   id: nat;
   name: text;
   quantity: nat;
 };
type Category = 
 record {
   items: vec PredefinedItem;
   name: text;
 };
service : {
  addItem: (text, text, text, nat) -> (Result);
  getItems: () -> (vec Item) query;
  getPredefinedCategories: () -> (vec Category) query;
  markItemCompleted: (nat, bool) -> (Result);
  removeItem: (nat) -> (Result);
  updateItemQuantity: (nat, nat) -> (Result);
}
